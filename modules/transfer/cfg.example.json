{
    "debug": true,
    "minStep": 30,
    "http": {
        "enabled": true,
        "listen": "0.0.0.0:6060"
    },
    "rpc": {
        "enabled": true,
        "listen": "0.0.0.0:8433"
    },
    "socket": {
        "enabled": true,
        "listen": "0.0.0.0:4444",
        "timeout": 3600
    },
    "judge": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "judge-00": "127.0.0.1:6080"
        }
    },
    "graph": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "graph-00": "127.0.0.1:6070"
        }
    },
    "tsdb": {
        "enabled": false,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "address": "127.0.0.1:8088"
    },
    "ims": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "period": 60,
        "address": "http://0.0.0.0:10812/ims_data_access/send_machine_metric_by_json.do",
        "falconToIms": {
            "cpu.busy": {
                "name": "cpu"
            },
            "load.1min": {
                "name": "cpu_min1"
            },
            "load.5min": {
                "name": "cpu_min5"
            },
            "mem.memused": {
                "name": "mem_app_total",
                "expression": "value/1048576"
            },
            "mem.swaptotal": {
                "name": "mem_swap_total",
                "expression": "value/1024"
            },
            "mem.swapused": {
                "name": "mem_swap_used",
                "expression": "value/1024"
            },
            "mem.memtotal": {
                "name": "mem_total",
                "expression": "value/1024"
            },
            "df.bytes.used.percent": {
                "name": "disk_used_rate",
                "tag": "mount"
            },
            "disk.io.util.max": {
                "name": "disk_util_max"
            },
            "net.if.in.dropped": {
                "name": "net_in_discard",
                "tag": "iface"
            },
            "net.if.in.errors": {
                "name": "net_in_err",
                "tag": "iface"
            },
            "net.if.in.bytes": {
                "name": "net_in_size",
                "expression": "value/1024",
                "tag": "iface"
            },
            "net.if.in.packets": {
                "name": "net_in_num",
                "tag": "iface"
            },
            "net.if.out.dropped": {
                "name": "net_out_discard",
                "tag": "iface"
            },
            "net.if.out.errors": {
                "name": "net_out_err",
                "tag": "iface"
            },
            "net.if.out.bytes": {
                "name": "net_out_size",
                "expression": "value/1024",
                "tag": "iface"
            },
            "net.if.out.packets": {
                "name": "net_out_num",
                "tag": "iface"
            },
            "switch.CpuUtilization": {
                "name": "cpu"
            },
            "switch.sessions.fw": {
                "name": "session"
            },
            "switch.if.InDiscards": {
                "name": "in_droppkt",
                "tag": "ifName"
            },
            "switch.if.InErrors": {
                "name": "in_errpkt",
                "tag": "ifName"
            },
            "switch.if.InPkts": {
                "name": "in_packet",
                "tag": "ifName"
            },
            "switch.if.In": {
                "name": "in_traffic",
                "expression": "value/1024",
                "tag": "ifName"
            },
            "switch.if.OutDiscards": {
                "name": "out_droppkt",
                "tag": "ifName"
            },
            "switch.if.OutErrors": {
                "name": "out_errpkt",
                "tag": "ifName"
            },
            "switch.if.OutPkts": {
                "name": "out_packet",
                "tag": "ifName"
            },
            "switch.if.Out": {
                "name": "out_traffic",
                "expression": "value/1024",
                "tag": "ifName"
            }
        }
    }
}